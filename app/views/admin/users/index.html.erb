<% provide :title, t(".title") %>
<h1><%= t ".title" %></h1>

<h2 class="left">
  <%= t ".search_f" %>
</h2>
<div class="row">
  <%= render "search_form" %>
</div>
<hr>

<h2 class="left">
  <%= t ".import_csv" %>
</h2>
<div class="row">
  <div class="col-md-5">
    <%= render "import_csv_form" %>
    <hr>
  </div>
</div>

<h1 class="left">
  <%= t ".users_list" %>
</h1>
<%= link_to t(".export_csv"), admin_users_path(format: :csv),
  class: "btn btn-primary" %>
<ul class="users">
  <%= render @users %>
</ul>
<%= paginate @users %>
